ainParts.some(part => part.length === 0)) return false;\r\n    return true;\r\n}   \r\n    \r\nfunction hideLoader() {\r\n    var containerLoader = $('.container-loader');\r\n    if ( !$(containerLoader).hasClass('elementor-element-edit-mode') )\r\n        $(containerLoader).css('display','none');\r\n}\r\n\r\n\r\nfunction checkFormFields()\r\n{\r\n     if ( !isValidated ) {\r\n        var first_name = $('#form-field-first_name').val();\r\n        var last_name = $('#form-field-last_name').val();\r\n        var email = $('#form-field-email').val();\r\n        var phone = $('#form-field-phone').val();\r\n            \r\n        \/\/ Fill in the billing fields\r\n        $('#billing_first_name').val(first_name);\r\n        $('#billing_last_name').val(last_name);\r\n        $('#billing_email').val(email);\r\n        $('#billing_phone').val(phone);\r\n        \r\n        var containerLoader = $('.container-loader');\r\n        \r\n        if ( !(first_name || last_name || email || phone) ) {\r\n            \r\n            if ( !$(containerLoader).hasClass('elementor-element-edit-mode') )\r\n                window.location = '\/sign-up\/'; \r\n            return;\r\n        }\r\n        \r\n        if ( !first_name || !last_name || !email || !phone ) {\r\n            \r\n            hideLoader();\r\n            $('.error .elementor-heading-title').html('Please fill in all fields.');\r\n            $('.container-form').css('display','flex');\r\n            return;\r\n        }\r\n        \r\n        if (!isValidEmail(email)) {\r\n            hideLoader();\r\n            $('.error .elementor-heading-title').html('Please enter a valid email.');\r\n            $('.container-form').css('display','flex');\r\n            return;\r\n        }\r\n        \r\n        checkEmailAndIfUserLoggedIn();\r\n     }\r\n     \r\n    \/\/  return false;\r\n    \r\n}\r\n\r\nfunction checkEmailAndIfUserLoggedIn() {\r\n    var email = $('#form-field-email').val();\r\n    \r\n    var containerLoader = $('.container-loader');\r\n\r\n    $(containerLoader).css('display','flex');\r\n\r\n    \/\/ Append the checkbox value to the WooCommerce checkout form\r\n    var textMessageAgreement = $('#form-field-textMessageAgreement').is(':checked') ? 'true' : 'false';\r\n    \r\n    $('<input>').attr({\r\n        type: 'hidden',\r\n        id: 'hidden_text_message_agreement',\r\n        name: 'text_message_agreement',\r\n        value: textMessageAgreement\r\n    }).appendTo('form.checkout');\r\n\r\n    $.ajax({\r\n        type: \"POST\",\r\n        dataType: 'json',\r\n        url: ajaxObj.ajaxUrl,\r\n        data: {\r\n            email: email,\r\n            action: 'user_status',\r\n            security: ajaxObj.security\r\n        },\r\n        success: function (data) {\r\n            \/\/ console.log('data: ', data);\r\n            if (data.data.logged_in) {\r\n                $('.error .elementor-heading-title').html('You are already logged in, <a href=\"https:\/\/dash.spines.com\">click here<\/a> to go to the platform.');\r\n                $('.error').removeClass('dce-visibility-element-hidden');\r\n                \/\/ $('.container-form').css('display','flex');\r\n                \r\n                hideLoader();\r\n                \/\/ console.log('data.data.logged_in: ', data.data.logged_in);\r\n            } else if (data.data.email_exists) {\r\n                $('.error .elementor-heading-title').html('It looks like this email is already associated with an account. If you\\'ve already signed up, please <a href=\"https:\/\/dash.spines.com\/loginPage\" style=\"white-space:nowrap;\">log in here.<\/a>');\r\n                $('.error').removeClass('dce-visibility-element-hidden');\r\n                \r\n                \/\/ $('.container-form').css('display','flex');\r\n                \r\n                $('.forgot-password-link').removeClass('dce-visibility-element-hidden'); \r\n                \r\n                hideLoader();\r\n                \/\/  console.log('data.logged_in: ', data.data.logged_in);\r\n            } else {\r\n                \r\n                \/\/ console.log('checkout click!! ');\r\n                isValidated = true;\r\n                submitCheckoutForm();\r\n                \/\/ $('.signup-form button[type=\"submit\"]').trigger('click');\r\n                \r\n            }\r\n        },\r\n        error: function () {\r\n\r\n            $('.error .elementor-heading-title').html(\"Oops! An error has occurred. We're working to fix it.\");\r\n            \r\n            $('.error-link').removeClass('dce-visibility-element-hidden'); \r\n            \r\n            \/\/ $('.container-form').css('display','flex');\r\n\r\n            hideLoader();\r\n        }\r\n    });\r\n}\r\n\r\nfunction submitCheckoutForm() {\r\n    \/\/ Attempt to click the primary button\r\n    if ($('.checkout-bottom .btn-submit').length) {\r\n        $('.checkout-bottom .btn-submit').click();\r\n    } else {\r\n        \/\/ Fallback to click the secondary button\r\n        $('#place_order').click();\r\n    }\r\n}\r\n\r\n})(jQuery);\r\n\r\n<\/script>\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-6fb96a71 ob-has-background-overlay elementor-widget elementor-widget-shortcode\" data-id=\"6fb96a71\" data-element_type=\"widget\" data-e-type=\"widget\" data-settings=\"{&quot;_ob_widget_stalker_use&quot;:&quot;no&quot;,&quot;_ob_poopart_use&quot;:&quot;yes&quot;,&quot;_ob_shadough_use&quot;:&quot;no&quot;,&quot;_ob_allow_hoveranimator&quot;:&quot;no&quot;}\" data-widget_type=\"shortcode.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t\t\t<div class=\"elementor-shortcode\"><\/div>\n\t\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t","protected":false},"excerpt":{"rendered":"<p>We&#8217;re setting things up, just a moment! Error container Go to the Homepage Forgot your password? Click here to reset it. Take the first